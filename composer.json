{
    "name": "pyncer/utility",
    "type": "library",
    "description": "Pyncer library containing common interfaces, traits, and classes.",
    "homepage": "https://pyncer.com/utility",
    "license": "Unlicense",
    "require": {
        "php": ">=8.1",
        "psr/container": "^2.0.2",
        "pyncer/core": "^1.1.6",
        "pyncer/exception": "^1.0",
        "pyncer/iterable": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Pyncer\\Utility\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/pyncerrc/pyncer-utility/issues",
        "source": "https://github.com/pyncerrc/pyncer-utility"
    },
    "extra" : {
        "branch-alias" : {
            "dev-main" : "1.1-dev"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    }
}
